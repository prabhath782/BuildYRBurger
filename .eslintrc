{
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["prettier"],

  "extends": ["airbnb", "prettier", "prettier/react"],
  "rules": {
    "no-console": 0,
    "no-unused-vars": 1,
    "no-underscore-dangle": 0,
    "react/jsx-filename-extension": [
      "error",
      { "extensions": [".jsx", ".js"] }
    ],
    "react/prefer-stateless-function": [0, { "ignorePureComponents": true }],
    "linebreak-style": 0,
    "react/prop-types": 0,
    "react/state": 0
  }
}
